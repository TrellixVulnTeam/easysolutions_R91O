<FileLoader>:
    filechooser: filechooser

    BoxLayout:
        size: root.size
        pos: root.pos
        orientation: "vertical"
        canvas.before:
            Color:
                rgba : rgba("#1034a6")
            Rectangle:
                pos: self.pos
                size:self.size
        FileChooserIconView:

            id: filechooser
            path : app.internalstorage
            multiselect: False

        BoxLayout:
            size_hint_y: None
            height: 30
            Button:
                text: "Cancel"


            Button:
                text: "Load"
                on_release: app.load(filechooser.path, filechooser.selection)
